<% provide(:title, (t '.subtitle')) %>
<div class="page-header"><h1><%= (t '.header.main') %></h1></div>

<div class="jumbotron col-md-10 user">
    <%= form_for @user, html: {class: "col-md-7"} do |f| %>
        <%= render 'form', f: f %>
        <%= f.submit (t '.button.submit'), class: "btn btn-md btn-success pull-right" %>
    <% end %>
</div>
