<% provide(:title, 'Nowy produkt') %>
<h3 class="text-left">Dodaj produkt</h3>
<div class="row">
  <div class="span6 offset1">
    <%= form_for(@product) do |f| %>
        <%= render 'form', f: f %>
        <%= f.submit "Zapisz produkt", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>