<% provide(:title, (t '.subtitle')) %>
<div class="hero-unit">
  <h3 class="text-left"><%= t '.header.main' %></h3>
  <div class="row">
    <div class="span7 offset1">
      <%= form_for @profil do |f| %>
          <%= render 'form', f: f %>
              <%= f.submit (t '.button.submit'), class: 'btn btn-medium btn-primary pull-right' %>
      <% end %>
    </div>
  </div>
</div>